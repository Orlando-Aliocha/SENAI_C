programa {
	funcao inicio() {
		inteiro vetor[3]
	    
		para (inteiro i = 0; i < 3; i++){ 
		    escreva("Digite o valor da posição ",i ,"\n")
		    leia(vetor[i])
		    }
	    escreva("Os valores digitados são: \n")
